{
  "root": true,
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "script"
  },
  "env": {
    "browser": true
  },
  "globals": {
    "M": "readonly"
  },
  "plugins": ["prettier"],
  "extends": ["eslint:recommended", "prettier"],
  "rules": {
    "prettier/prettier": "error",
    "no-unused-vars": "off"
  }
}
